@using TP_Final_Grupo_06.Models;
@model Local


@if (ViewBag.resultado = null)
{
    ViewBag.resultado;
}

else
{



@{
    ViewBag.Title = "Local";
}

<h2>Local</h2>

<div class="card" style="width: 32%;height:5%;margin-top: 4%;margin-left:1%;float: left;">
<a><img class="card-img-top" src="@ViewBag.Local.urlimagen" alt="Card image cap"></a>
</div>

<div>
    <h1> @ViewBag.Local.nombre_local </h1>

</div>

@ViewBag.id_local

@ViewBag.Local.descripcion
@ViewBag.Local.piso
@ViewBag.Local.id_rubro
@ViewBag.Local.urlimagen

    }